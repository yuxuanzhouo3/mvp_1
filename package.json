{
  "name": "personalink",
  "version": "1.0.0",
  "description": "AI-driven personality-based social matching platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
