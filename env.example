# Payment Configuration (optional)
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
STRIPE_SECRET_KEY=sk_test_your_stripe_key

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379

# Environment
NODE_ENV=development 